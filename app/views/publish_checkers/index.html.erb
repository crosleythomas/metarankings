<h1>Listing publish_checkers</h1>

<table>
  <thead>
    <tr>
      <th>Website</th>
      <th>Publish tok</th>
      <th>Week</th>
      <th>Year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @publish_checkers.each do |publish_checker| %>
      <tr>
        <td><%= publish_checker.website %></td>
        <td><%= publish_checker.publish_tok %></td>
        <td><%= publish_checker.week %></td>
        <td><%= publish_checker.year %></td>
        <td><%= link_to 'Show', publish_checker %></td>
        <td><%= link_to 'Edit', edit_publish_checker_path(publish_checker) %></td>
        <td><%= link_to 'Destroy', publish_checker, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Publish checker', new_publish_checker_path %>
