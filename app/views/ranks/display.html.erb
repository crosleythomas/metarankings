
<h1>Listing ranks</h1>

<table>
  <thead>
    <tr>
      <th>Website</th>
      <th>Team</th>
      <th>Rank</th>
      <th>Week</th>
      <th>Year</th>
      <th>Description</th>
      <th>Article link</th>
      <th>Published</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% if !@ranks.nil? %>
      <% @ranks.each do |rank| %>
        <tr>
          <td><%= rank.website %></td>
          <td><%= rank.team %></td>
          <td><%= rank.rank %></td>
          <td><%= rank.week %></td>
          <td><%= rank.year %></td>
          <td><%= rank.description %></td>
          <td><%= rank.article_link %></td>
          <td><%= rank.published %></td>
          <td><%= link_to 'Show', rank %></td>
          <td><%= link_to 'Edit', edit_rank_path(rank) %></td>
          <td><%= link_to 'Destroy', rank, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    <% end %>
  </tbody>
</table>

<br>
