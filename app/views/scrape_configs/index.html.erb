<h1>Listing scrape_configs</h1>

<table>
  <thead>
    <tr>
      <th>Website</th>
      <th>Url</th>
      <th>Rank xpath</th>
      <th>Rank regex</th>
      <th>Team xpath</th>
      <th>Team regex</th>
      <th>Description xpath</th>
      <th>Description regex</th>
      <th>Update checker xpath</th>
      <th>Update checker regex</th>
      <th>Week</th>
      <th>Year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @scrape_configs.each do |scrape_config| %>
      <tr>
        <td><%= scrape_config.website %></td>
        <td><%= scrape_config.url %></td>
        <td><%= scrape_config.rank_xpath %></td>
        <td><%= scrape_config.rank_regex %></td>
        <td><%= scrape_config.team_xpath %></td>
        <td><%= scrape_config.team_regex %></td>
        <td><%= scrape_config.description_xpath %></td>
        <td><%= scrape_config.description_regex %></td>
        <td><%= scrape_config.update_checker_xpath %></td>
        <td><%= scrape_config.update_checker_regex %></td>
        <td><%= scrape_config.week %></td>
        <td><%= scrape_config.year %></td>
        <td><%= link_to 'Show', scrape_config %></td>
        <td><%= link_to 'Edit', edit_scrape_config_path(scrape_config) %></td>
        <td><%= link_to 'Destroy', scrape_config, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <td><%= link_to 'Clone', scrape_config_path(scrape_config) + "/clone" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Scrape config', new_scrape_config_path %>
